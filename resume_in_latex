%----------------------------------------------------------------------------------------
%	EXPERIENCE
%----------------------------------------------------------------------------------------
\vspace{-10pt}
\cvsect{Experience}
\begin{entrylist}
	\entry
        {\textbf{entri.app}}
		{Backend Developer - Intern (\textit{04/2023 - Present})}
        {}
		{
            \begin{itemize}
                \item Developed backend for \textbf{Gratitudes-Entri}, a thanksgiving app - aimed to foster a culture of gratitude and appreciation within the company.
                \item Users used their slack accounts to login to this app which made them to use the app with ease. 
                \item Used \textit{\textbf{FastAPI}} as backend application, \textit{\textbf{MongoDB}} as the database, \textit{\textbf{Redis}} for caching, \textbf{Slack OAuth} and \textbf{JWT} for authentication and \textit{\textbf{Docker}} for its containerization,
                \item Developed backend for \textbf{Entri-DeviceTracker}, an app aimed to track devices issued for testing and other purposes within the company, automating the process rather than manually maintaining the records.
                \item Used \textbf{\textit{Django}} as the backend application, \textit{\textbf{PostgresSQL}} as the database for the app.
                \item Leveraged exposure to \textbf{Celery} and \textbf{RabbitMQ} in the meantime.
                \item Was part of an \textbf{ETL} project , using \textbf{Airbyte} for data extraction, \textbf{DBT} for data transformation, and \textbf{Airflow} for data synchronization.

            \end{itemize}
        }
\end{entrylist}

